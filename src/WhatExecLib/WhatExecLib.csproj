<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net8.0;net9.0;netstandard2.0</TargetFrameworks>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>13</LangVersion>
        <AssemblyName>AlastairLundy.WhatExecLib</AssemblyName>
        <RootNamespace>AlastairLundy.WhatExecLib</RootNamespace>
        <PackageId>AlastairLundy.WhatExecLib</PackageId>
        <Title>WhatExecLib</Title>
        <Authors>Alastair Lundy</Authors>
        <Copyright>Copyright (c) Alastair Lundy</Copyright>
        <RepositoryUrl>https://github.com/alastairlundy/whatexec</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AlastairLundy.DotPrimitives" Version="3.3.0" />
        <PackageReference Include="Polyfill" Version="8.9.0" Condition="'$(TargetFramework)' == 'netstandard2.0'"/>
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\WhatExecLib.Abstractions\WhatExecLib.Abstractions.csproj" />
    </ItemGroup>

</Project>
